{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jasmine": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": [2, "always"],
        "quotes": [2, "double"],
        "indent": ["error", 4],
        "space-before-function-paren": ["off", "never"]
    },
    "parser":"babel-eslint"
}